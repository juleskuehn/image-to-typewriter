body
	padding 0
	margin 0
	height 100%
	position absolute
	width 100%

#options
	position relative
	
#keystoneCanvasContainer
	position absolute
	top 160px
	left 0
	
#keystones
	position relative
	width 0
	height 0
	div
		position absolute
		background rgba(255,0,0,0.15)
		width 19px
		height 19px
		border solid 1px red
		border-radius 10px
		div.v
			width 1px
			height 19px
			position absolute
			left 9px
			top 0
			background red
			border none
		div.h
			width 19px
			height 1px
			position absolute
			left 0
			top 9px
			background red
			border none